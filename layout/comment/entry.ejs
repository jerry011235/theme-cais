<% if (theme.comment.changyan && theme.comment.changyan.appid && theme.comment.changyan.conf) { %>
<%- partial('comment/changyan', { item: item }) %>
<% } else if (theme.comment.youyan) { %>
<%- partial('comment/youyan', { item: item }) %>
<% } else if (theme.comment.gitment) {
    let gitment = theme.comment.gitment
if (gitment.owner && gitment.repo && gitment.oauth.client_id && gitment.oauth.client_secret) { %>
