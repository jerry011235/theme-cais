<% }
} else if (theme.comment.disqus) { %>
<%- partial('comment/disqus') %>
<% } else if (theme.comment.valine && theme.comment.valine.appId && theme.comment.valine.appKey) { %>
<%- partial('comment/valine') %>
<% } %>